<fieldset>
    <legend>Windows</legend>
    <BUTTON class="btn btn-default" class=button onclick="Run('notepad')">记事本</BUTTON>
    <BUTTON class="btn btn-default" class=button onclick="Cmd('notepad')">Notepad</BUTTON>
    <BUTTON class="btn btn-default" class=button onclick="Run('mspaint')">画图板</BUTTON>
    <BUTTON class="btn btn-default" class=button onclick="Run('calc')">计算器</BUTTON>
    <BUTTON class="btn btn-default" class=button onclick="Run('cmd')">cmd</BUTTON>
    <BUTTON class="btn btn-default" class=button onclick="Run('write')">写字板</BUTTON>
    <BUTTON class="btn btn-default" class=button onclick="Exe('%windir%\\system32\\SnippingTool.exe')">截图</BUTTON>
    <BUTTON class="btn btn-default" class=button onclick="Exe('edit_hosts.bat')">hosts</BUTTON>
</fieldset>

<fieldset>
    <legend>Folders</legend>
    <BUTTON class="btn btn-default" class=button onclick="Run('explorer')">库</BUTTON>
